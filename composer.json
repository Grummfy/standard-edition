{
    "name": "atoum/standard-edition",
    "keywords": ["TDD", "atoum", "test", "unit testing", "atoum-edition"],
    "homepage": "http://www.atoum.org",
    "require": {
        "atoum/atoum": "^2.9|^3.0",
        "atoum/stubs": "^2.5",
        "atoum/visibility-extension": "^1.2",
        "atoum/config-extension": "^0.1",
        "atoum/reports-extension": "^2.4"
    },
    "license": "MIT",
    "autoload": {
        "files": [ "autoload.php" ]
    },
    "suggest": {
        "atoum/autoloop-extension": "Automaticly run your tests in atoum's loop mode when files are changed"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.12"
    }
}
